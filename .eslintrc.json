{
    "extends": [ "airbnb-base","prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier":"error",
        "no-unused-vars":"warn",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off"
    }
}
